{
  "name": "abtest-kit",
  "version": "0.0.2",
  "description": "Lightweight A/B testing SDK with multiple strategies (random, CRC32, custom) and optional React integration. Supports Baidu Tongji and standalone usage.",
  "keywords": [
    "ab-test",
    "a/b-test",
    "split-test",
    "experiment",
    "baidu-tongji",
    "react",
    "typescript"
  ],
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "robuild",
    "dev": "robuild --watch",
    "play": "pnpm --filter playground dev",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.15",
    "changelogen": "^0.6.2",
    "concurrently": "^9.2.1",
    "git-cz": "^4.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "robuild": "^0.0.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
